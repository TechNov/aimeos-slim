{
	"name": "aimeos/aimeos-slim",
	"description": "Professional, full-featured and ultra fast SlimPHP e-commerce package for (TV-approved) online shops",
	"homepage": "https://aimeos.org/SlimPHP",
	"type": "library",
	"license": "LGPL-3.0-or-later",
	"keywords": ["aimeos", "slim", "slimphp", "e-commerce", "ecommerce", "TV", "shop"],
	"support": {
		"source": "https://github.com/Aimeos/aimeos-slim",
		"issues": "https://github.com/Aimeos/aimeos-slim/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"aimeos/aimeos-core": "~2020.01",
		"aimeos/ai-admin-jqadm": "~2020.01",
		"aimeos/ai-admin-jsonadm": "~2020.01",
		"aimeos/ai-client-html": "~2020.01",
		"aimeos/ai-client-jsonapi": "~2020.01",
		"aimeos/ai-controller-jobs": "~2020.01",
		"aimeos/ai-gettext": "~2020.01",
		"aimeos/ai-monolog": "~2020.01",
		"aimeos/ai-swiftmailer": "~2020.01",
		"aimeos/ai-slim": "~2020.01",
		"aimeos/ai-twig": "~2020.01",
		"slim/twig-view": "~2.0",
		"slim/slim": "~3.3",
		"akrabat/ip-address-middleware": "^0.5"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0||~8.0",
		"php-coveralls/php-coveralls": "~2.0"
	},
	"autoload": {
		"psr-4": { "Aimeos\\Slim\\": "src/" }
	}
}
