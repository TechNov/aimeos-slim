{
	"name": "aimeos/aimeos-slim",
	"description": "Slim online shop package and e-commerce solution for high performance shops",
	"homepage": "https://aimeos.org/SlimPHP",
	"type": "library",
	"license": "LGPL-3.0-or-later",
	"keywords": ["aimeos", "slim", "e-commerce", "ecommerce", "shop", "portal", "market place"],
	"support": {
		"source": "https://github.com/Aimeos/aimeos-slim",
		"issues": "https://github.com/Aimeos/aimeos-slim/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/aimeos/php-coveralls"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"aimeos/aimeos-core": "~2018.07",
		"aimeos/ai-admin-jqadm": "~2018.07",
		"aimeos/ai-admin-jsonadm": "~2018.07",
		"aimeos/ai-client-html": "~2018.07",
		"aimeos/ai-client-jsonapi": "~2018.07",
		"aimeos/ai-controller-jobs": "~2018.07",
		"aimeos/ai-gettext": "~2018.07",
		"aimeos/ai-monolog": "~2018.07",
		"aimeos/ai-swiftmailer": "~2018.07",
		"aimeos/ai-slim": "~2018.07",
		"aimeos/ai-twig": "~2018.07",
		"slim/twig-view": "~2.0",
		"slim/slim": "~3.3",
		"akrabat/ip-address-middleware": "^0.5"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master",
		"phpunit/phpunit": "^4.8.35||~5.0||~6.0"
	},
	"autoload": {
		"psr-4": { "Aimeos\\Slim\\": "src/" }
	}
}
